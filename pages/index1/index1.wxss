.container {
  width: 100%;
}

.body {
  background-color: #EEF3FA;
}

.page-body {
  width: 100%;
  height: 320rpx;
}

.page-body-img {
  width: 100%;
  height: 100%;
}

#wrap {
  padding: 0;
}

/* nav */
.nav {
  width: 100%;
  display: flex;
  justify-content: space-around;
}

.nav_list {
  height: 320rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
}

.nav_list>view {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;

}

.nav_list1 {
  background-color: #4799f5;
}

.nav_list2 {
  background-color: #00b57b;
}

.nav_list3 {
  background-color: #f54752;
}

.nav_list>view>image {
  width: 60rpx;
  height: 60rpx;
}

.nav_list>text {
  font-size: 12px;
  padding-top: 10rpx;
  text-align: center;
  color: #5a4f51;
}

/* main部分 */
.main {
  padding: 13rpx 38rpx;
}

.header {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.header_left {
  display: flex;
  width: 44%;
  align-items: center;
}

.header_left>image {
  width: 63rpx;
  height: 63rpx;
}

.header_left>text {
  line-height: 93rpx;
  font-size: 42rpx;
  font-weight: 500;
  color: #525253;
  padding-left: 10rpx;
  box-sizing: border-content;
}

.header_right {
  width: 27%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.header_right>text {
  font-size: 35rpx;
  color: #b6bac1;
  line-height: 93rpx;
}
.header_right>image {
  width: 30rpx;
  height: 30rpx;
}
